      "settings": {
        "foreground": "#52a9ff"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ff5c61"
      }
    },
    {
      "name": "Attribute Value",
      "scope": [
