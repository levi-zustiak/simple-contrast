{
  "name": "Simple Contrast",
  "type": "dark",
  "colors": {
    "editor.background": "#141617",
    "editor.foreground": "#ededee",
    "sideBar.background": "#141617",
    "sideBar.foreground": "#ededee",
    "sideBarTitle.foreground": "#ededee",
    "editorGroupHeader.tabsBackground": "#141617",
    "editorGroup.border": "#282b2d",
    "tab.activeBackground": "#2a2a2a",
    "tab.inactiveBackground": "#141617",
    "tab.border": "#141617",
    "tab.hoverBackground": "#212121",
    "activityBar.background": "#212121",
    "activityBarBadge.background": "#52a9ff",
    "activityBarBadge.foreground": "#212121",
    "editor.lineHighlightBackground": "#2a2a2a",
    "editor.selectionBackground": "#343434",
    "statusBar.background": "#212121",
    "button.background": "#52a9ff",
    "button.foreground": "#141617",
    "button.hoverBackground": "#52a9ffe8",
    "dropdown.background": "#2a2a2a",
    "dropdown.listBackground": "#212121",
    "dropdown.activeBorder": "#ff0000",
    "dropdown.foreground": "#ededee",
    "input.background": "#2a2a2a",
    "inputOption.activeBorder": "#ff0000",
    "progressBar.background": "#c072f8",
    "quickInput.background": "#212121",
    "quickInput.foreground": "#ededee",
    "quickInputList.focusBackground": "#343434",
    "quickInputList.focusForeground": "#ededee",
    "keybindingLabel.background": "#c072f8",
    "keybindingLabel.foreground": "#141617"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#81888d"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal", "invalid.broken"],
      "settings": {
        "foreground": "#ff5c61"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#ededee"
      }
    },
    {
      "name": "Symbols, Markup Heading",
      "scope": [
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#ededee"
      }
    },
    {
      "name": "Imports & Exports",
      "scope": [
        "keyword.control.from",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default"
      ],
      "settings": {
        "foreground": "#52a9ff"
      }
    },
    {
      "name": "Import Value",
      "scope": ["variable.other.readwrite.alias", "meta.import"],
      "settings": {
        "foreground": "#ededee"
      }
    },
    {
      "name": "Import Path",
      "scope": ["meta.import string"],
      "settings": {
        "foreground": "#ff5c61"
      }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol", "constant.other.key"],
      "settings": {
        "foreground": "#ff5c61"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#c072f8"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#fcaf4a"
      }
    },
    {
      "name": "Object Property",
      "scope": ["meta.object-literal.key", "variable.object.property"],
      "settings": {
        "foreground": "#ededee"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.operator"],
      "settings": {
        "foreground": "#81888d"
      }
    },
    {
      "name": "Keyword Control",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#52a9ff"
      }
    },
    {
      "name": "Keyword Operator Logical",
      "scope": ["keyword.operator.logical"],
      "settings": {
        "foreground": "#ededee"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": ["keyword.other.template", "keyword.other.substitution"],
      "settings": {
        "foreground": "#81888d"
      }
    },
    {
      "name": "Object Name",
      "scope": ["variable.other.object"],
      "settings": {
        "foreground": "#ededee"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#81888d"
      }
    },
    {
      "name": "Special Punctuation",
      "scope": [
        "punctuation.definition.binding-pattern.object",
        "punctuation.definition.binding-pattern.array"
      ],
      "settings": {
        "foreground": "#ededee"
      }
    },
    {
      "name": "Punctuation Accessor",
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "#ededee"
      }
    },
    {
      "name": "Operator Spread",
      "scope": ["keyword.operator.spread", "keyword.operator.rest"],
      "settings": {
        "foreground": "#52a9ff"
      }
    },
    {
      "name": "HTML",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#ededee"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#52a9ff"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#ededee"
      }
    },
    {
      "name": "Variable Constant",
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#ededee"
      }
    },
    {
      "name": "Variable Object Property",
      "scope": ["variable.other.constant.property"],
      "settings": {
        "foreground": "#52a9ff"
      }
    },
    {
      "name": "Function",
      "scope": ["storage.type.function"],
      "settings": {
        "foreground": "#52a9ff"
      }
    },
    {
      "name": "Arrow Function",
      "scope": ["storage.type.function.arrow"],
      "settings": {
        "foreground": "#c072f8"
      }
    },
    {
      "name": "Function Name, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "meta.function-call entity.name.function",
        "keyword.other.special-method",
        "meta.block-level"
      ],
      "settings": {
        "foreground": "#c072f8"
      }
    },
    {
      "name": "Support Function",
      "scope": ["support.function", "meta.function-call entity.name.function"]
    },
    {
      "name": "Function Args",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#ededee"
      }
    },
    {
      "name": "Function Punctuation",
      "scope": [
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end"
